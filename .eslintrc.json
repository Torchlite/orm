{
	"parserOptions": {
		"ecmaVersion": 8,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		}
	},
	"env": {
		"browser": false,
		"node": true,
		"amd": false,
		"mocha": true,
		"jasmine": false,
		"phantomjs": false,
		"jquery": false,
		"prototypejs": false,
		"shelljs": false,
		"es6": true
	},
	"globals": {},
	"plugins": [],
	"rules": {
		"comma-dangle": [2, "never"],
		"no-cond-assign": [2, "always"],
		"no-console": 0,
		"no-constant-condition": 2,
		"no-control-regex": 2,
		"no-debugger": 2,
		"no-dupe-keys": 2,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-parens": 0,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 0,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-reserved-keys": 0,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"no-unused-vars": [2, { "args": "none" }],
		"use-isnan": 2,
		"valid-jsdoc": 0,
		"valid-typeof": 2,
		"block-scoped-var": 2,
		"complexity": 0,
		"consistent-return": 0,
		"curly": 2,
		"default-case": 0,
		"dot-notation": 2,
		"eqeqeq": [2, "smart"],
		"guard-for-in": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-div-regex": 2,
		"no-else-return": 1,
		"no-labels": "error",
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-fallthrough": 0,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-octal-escape": 2,
		"no-process-env": 0,
		"no-proto": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-unused-expressions": 2,
		"no-throw-literal": 2,
		"no-void": 2,
		"no-warning-comments": 1,
		"no-with": 2,
		"radix": 2,
		"vars-on-top": 2,
		"wrap-iife": [2, "inside"],
		"yoda": 2,
		"no-extra-strict": 0,
		"strict": [0, "safe"],
		"no-catch-shadow": 0,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow": 0,
		"no-shadow-restricted-names": 2,
		"no-undef": 2,
		"no-undef-init": 2,
		"no-undefined": 0,
		"no-use-before-define": 2,
		"handle-callback-err": 0,
		"no-mixed-requires": 0,
		"no-new-require": 0,
		"no-path-concat": 0,
		"no-process-exit": 0,
		"no-restricted-modules": 0,
		"no-sync": 0,
		"brace-style": 2,
		"camelcase": 0,
		"comma-spacing": [2, { "before": false, "after": true }],
		"comma-style": [2, "last"],
		"consistent-this": 0,
		"eol-last": 2,
		"func-names": 0,
		"func-style": [2, "declaration", { "allowArrowFunctions": true }],
		"key-spacing": [2, { "beforeColon": false, "afterColon": true }],
		"max-nested-callbacks": 0,
		"new-cap": 0,
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-inline-comments": 0,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": [2, { "max": 2 }],
		"no-nested-ternary": 2,
		"no-new-object": 2,
		"semi-spacing": 2,
		"no-spaced-func": 2,
		"no-ternary": 0,
		"no-trailing-spaces": 2,
		"no-underscore-dangle": 0,
		"no-wrap-func": 0,
		"one-var": [2, "never"],
		"operator-assignment": 0,
		"padded-blocks": [2, "never"],
		"quote-props": [2, "as-needed"],
		"quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
		"semi": [2, "always"],
		"sort-vars": 0,
		"space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
		"keyword-spacing": ["error", { "before": true }],
		"space-before-blocks": 2,
		"object-curly-spacing": [2, "always"],
		"array-bracket-spacing": [2, "never"],
		"computed-property-spacing": [2, "never"],
		"space-in-brackets": 0,
		"space-in-parens": [2, "never"],
		"space-infix-ops": [2, { "int32Hint": false }],
		"space-unary-ops": 2,
		"spaced-comment": [2, "always"],
		"wrap-regex": 0,
		"no-var": 2,
		"generator-star-spacing": 2,
		"max-depth": 0,
		"max-len": 0,
		"max-params": 0,
		"max-statements": 0,
		"no-bitwise": 0,
		"no-plusplus": 0
	}
}
